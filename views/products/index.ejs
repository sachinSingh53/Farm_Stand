<% layout('layout/boilerPlate') %>
    <h1 class="text-center mb-3">All Products</h1>
    <!-- <div class="text-center mb-4"> <a class="btn btn-warning " href="/products/new">Add a new Product</a></div> -->
   
    <ol>
        <% for(let product of products){ %>
            
           <div class="list-group text-center ">
           
            <li class="list-group-item text-center"> <a href="/products/<%= product._id %>"><%= product.name %></a></li>
            
            </div>
        <% } %>
        

    </ol>

    
